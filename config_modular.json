{
  "system": {
    "fps": 30.0,
    "frame_skip": 1,
    "logging_level": "INFO"
  },
  "face_detection": {
    "detection_confidence": 0.3,
    "mesh_confidence": 0.3,
    "model_selection": 1
  },
  "face_tracking": {
    "iou_threshold": 0.3,
    "max_frames_missing": 20,
    "min_session_duration": 0.5
  },
  "pose_estimation": {
    "estimator_type": "mediapipe",
    "yaw_multiplier": 1.5,
    "pitch_multiplier": 2.0
  },
  "zone_mapping": {
    "mapper_type": "bakery",
    "config_path": null
  },
  "visualization": {
    "display_output": true,
    "save_output": false,
    "output_path": "output_processed.mp4"
  },
  "analytics": {
    "console_output": true,
    "database_output": true,
    "json_output": true,
    "db_path": "gaze_analytics.db",
    "json_output_dir": "analytics_output",
    "verbose": true
  },
